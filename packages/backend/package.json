{
  "name": "@business-app/backend",
  "version": "1.0.0",
  "private": true,
  "main": "src/server.ts",
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate --schema=src/prisma/schema.prisma && tsc",
    "start": "prisma migrate deploy --schema=src/prisma/schema.prisma && node dist/server.js",
    "postinstall": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/prisma/schema.prisma",
    "prisma:seed": "tsx src/prisma/seed.ts",
    "seed:prod": "tsx src/prisma/seed-production.ts",
    "seed:grain": "tsx src/prisma/seed-grain-entities.ts",
    "seed:grain-data": "tsx src/prisma/seed-grain-data.ts",
    "backup": "node scripts/backup-database.js",
    "restore": "node scripts/restore-database.js",
    "list-backups": "node scripts/list-backups.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-s3": "^3.968.0",
    "@business-app/shared": "*",
    "@prisma/client": "^5.8.1",
    "@types/multer": "^2.0.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "pdf-poppler": "^0.2.3",
    "rate-limit-redis": "^4.3.1",
    "resend": "^6.7.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.6.1",
    "stripe": "^20.1.2",
    "web-push": "^3.6.7",
    "yahoo-finance2": "^3.11.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/web-push": "^3.6.4",
    "prisma": "^5.8.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
